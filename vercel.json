{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "framework": null,
  "rewrites": [
    {
      "source": "/api/field-notes",
      "destination": "/api/field-notes.js"
    },
    {
      "source": "/api/field-notes/:id",
      "destination": "/api/field-notes/[id].js"
    },
    {
      "source": "/:id",
      "destination": "/field-note.html"
    }
  ],
  "headers": [
    {
      "source": "/assets/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
